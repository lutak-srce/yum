[puppetlabs-products]
name=Puppet Labs Products Fedora 18 - $basearch
baseurl=http://yum.puppetlabs.com/fedora/f18/products/$basearch
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-puppetlabs
enabled=1
gpgcheck=1
priority=<%= @priority %>
<% if @exclude.to_s != '' -%>
exclude = <%= @exclude.join(' ') %>
<% end -%>

