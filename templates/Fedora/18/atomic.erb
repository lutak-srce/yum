# Name: Atomic Rocket Turtle RPM Repository for Fedora Core 18 - 
# URL: http://www.atomicrocketturtle.com/
# Note: This isn't covered by ASL support. -Scott
[atomic]
name = Fedora Core $releasever - atomicrocketturtle.com
mirrorlist = http://www.atomicorp.com/mirrorlist/atomic/fedora-18-$basearch
#mirrorlist = http://www.atomicorp.com/channels/atomic/fedora/18/mirrors-atomic
enabled = 1
protect = 0
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY.art.txt
gpgcheck = 1
priority=<%= @priority %>
<% if @exclude.to_s != '' -%>
exclude = <%= @exclude.join(' ') %>
<% end -%>
