[elrepo-kernel]
name=ELRepo.org Community Enterprise Linux Kernel Repository - el10
<% if @baseurl -%>
baseurl=<%= @baseurl %>
<% else -%>
baseurl=http://elrepo.org/linux/kernel/el10/$basearch/
        http://mirrors.coreix.net/elrepo/kernel/el10/$basearch/
        http://mirror.rackspace.com/elrepo/kernel/el10/$basearch/
        http://linux-mirrors.fnal.gov/linux/elrepo/kernel/el10/$basearch/
mirrorlist=http://mirrors.elrepo.org/mirrors-elrepo-kernel.el10
<% end -%>
enabled=1
countme=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-elrepo.org
priority=<%= @priority %>
<% unless @exclude.empty? -%>
exclude=<%= @exclude.join(' ') %>
<% end -%>
<% unless @include.empty? -%>
includepkgs=<%= @include.join(' ') %>
<% end -%>
