# Name: Atomic Rocket Turtle RPM Repository for CentOS / Red Hat Enterprise Linux 8 - 
# URL: http://www.atomicrocketturtle.com/
[atomic]
name = Rocky / Red Hat Enterprise Linux $releasever - atomic
<% if @baseurl -%>
baseurl = <%= @baseurl %>
<% else -%>
mirrorlist = https://updates.atomicorp.com/channels/mirrorlist/atomic/rocky-$releasever-$basearch
<% end -%>
enabled = 1
protect = 0
gpgkey = https://www.atomicorp.com/RPM-GPG-KEY.atomicorp.txt
gpgcheck = 1
priority = <%= @priority %>
<% unless @exclude.empty? -%>
exclude = <%= @exclude.join(' ') %>
<% end -%>
<% unless @include.empty? -%>
includepkgs = <%= @include.join(' ') %>
<% end -%>
