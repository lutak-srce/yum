# Almost Stable, release candidates for [atomic]
[atomic-testing]
name = Rocky / Red Hat Enterprise Linux $releasever - atomic - (Testing)
<% if @baseurl -%>
baseurl = <%= @baseurl %>
<% else -%>
mirrorlist = https://updates.atomicorp.com/channels/mirrorlist/atomic-testing/rocky-$releasever-$basearch
<% end -%>
enabled = 1
protect = 0
gpgkey = https://www.atomicorp.com/RPM-GPG-KEY.atomicorp.txt
gpgcheck = 1
priority = <%= @priority %>
<% unless @exclude.empty? -%>
exclude = <%= @exclude.join(' ') %>
<% end -%>
<% unless @include.empty? -%>
includepkgs = <%= @include.join(' ') %>
<% end -%>
