[elastic-<%= @version %>]
name=Elastic repository for <%= @version %> packages
<% if @baseurl -%>
baseurl=<%= @baseurl %>
<% else -%>
baseurl=https://artifacts.elastic.co/packages/<%= @version %>/yum
<% end -%>
gpgcheck=1
gpgkey=https://artifacts.elastic.co/GPG-KEY-elasticsearch
enabled=1
autorefresh=1
type=rpm-md
