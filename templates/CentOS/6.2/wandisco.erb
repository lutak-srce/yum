[WANdisco]
name=WANdisco Repo (SVN <%= @version %>)
enabled=1
baseurl=http://opensource.wandisco.com/rhel/6/svn-<%= @version %>/RPMS/$basearch/
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-WANdisco
priority=<%= @priority %>
<% if @exclude.to_s != '' -%>
exclude=<%= @exclude.to_a.join(' ') %>
<% end -%>
<% if @include.to_s != '' -%>
includepkgs=<%= @include.to_a.join(' ') %>
<% end -%>
