[ownCloud_ce_<%= @version %>]
name=ownCloud Server Version <%= @version %> (CentOS_6)
type=rpm-md
baseurl=https://download.owncloud.org/download/repositories/<%= @version %>/CentOS_6/
gpgcheck=1
gpgkey=https://download.owncloud.org/download/repositories/<%= @version %>/CentOS_6/repodata/repomd.xml.key
enabled=1
priority = <%= @priority %>
<% unless @exclude.empty? -%>
exclude = <%= @exclude.join(' ') %>
<% end -%>
<% unless @include.empty? -%>
includepkgs=<%= @include.join(' ') %>
<% end -%>
