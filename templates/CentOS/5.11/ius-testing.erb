[ius-testing]
name=IUS Community Packages for Enterprise Linux 5 - $basearch - Testing
#baseurl=http://dl.iuscommunity.org/pub/ius/testing/Redhat/5/$basearch
mirrorlist=http://dmirr.iuscommunity.org/mirrorlist/?repo=ius-el5-testing&arch=$basearch
failovermethod=priority
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/IUS-COMMUNITY-GPG-KEY
priority=<%= @priority %>
<% if @exclude.to_s != '' -%>
exclude = <%= @exclude.join(' ') %>
<% end -%>

