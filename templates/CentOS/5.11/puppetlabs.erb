[puppetlabs-products]
name=Puppet Labs Products El 5 - $basearch
baseurl=http://yum.puppetlabs.com/el/5/products/$basearch
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-puppetlabs
enabled=1
gpgcheck=1
priority=<%= @priority %>
<% unless @exclude.empty? -%>
exclude = <%= @exclude.join(' ') %>
<% end -%>

