[UMD-2-base]
name=UMD 2 base (SL6)
baseurl=http://repository.egi.eu/sw/production/umd/2/sl6/$basearch/base
protect=1
enabled=1
# To use priorities you must have yum-priorities installed
gpgcheck=1
gpgkey=http://emisoft.web.cern.ch/emisoft/dist/EMI/2/RPM-GPG-KEY-emi http://repo-rpm.ige-project.eu/RPM-GPG-KEY-IGE
priority=<%= @priority %>
<% if @exclude.to_s != '' -%>
exclude=<%= @exclude.to_a.join(' ') %>
<% end -%>
<% if @include.to_s != '' -%>
includepkgs=<%= @include.to_a.join(' ') %>
<% end -%>

