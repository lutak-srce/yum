[ius-archive]
name=IUS Community Packages for Enterprise Linux 6 - $basearch - Archive
#baseurl=http://dl.iuscommunity.org/pub/ius/archive/Redhat/6/$basearch
mirrorlist=http://dmirr.iuscommunity.org/mirrorlist/?repo=ius-el6-archive&arch=$basearch
failovermethod=priority
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/IUS-COMMUNITY-GPG-KEY
priority=<%= @priority %>
<% if @exclude.to_s != '' -%>
exclude=<%= @exclude.to_a.join(' ') %>
<% end -%>
<% if @include.to_s != '' -%>
includepkgs=<%= @include.to_a.join(' ') %>
<% end -%>

[ius-archive-debuginfo]
name=IUS Community Packages for Enterprise Linux 6 - $basearch - Archive Debug
#baseurl=http://dl.iuscommunity.org/pub/ius/archive/Redhat/6/$basearch/debuginfo
mirrorlist=http://dmirr.iuscommunity.org/mirrorlist/?repo=ius-el6-archive-debuginfo&arch=$basearch
failovermethod=priority
<% if @debuginfo -%>
enabled=1
<% else -%>
enabled=0
<% end -%>
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/IUS-COMMUNITY-GPG-KEY
priority=<%= @priority %>
<% if @exclude.to_s != '' -%>
exclude=<%= @exclude.to_a.join(' ') %>
<% end -%>
<% if @include.to_s != '' -%>
includepkgs=<%= @include.to_a.join(' ') %>
<% end -%>

[ius-archive-source]
name=IUS Community Packages for Enterprise Linux 6 - $basearch - Archive Source
#baseurl=http://dl.iuscommunity.org/pub/ius/archive/Redhat/6/SRPMS
mirrorlist=http://dmirr.iuscommunity.org/mirrorlist/?repo=ius-el6-archive-source&arch=source
failovermethod=priority
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/IUS-COMMUNITY-GPG-KEY
priority=<%= @priority %>
<% if @exclude.to_s != '' -%>
exclude=<%= @exclude.to_a.join(' ') %>
<% end -%>
<% if @include.to_s != '' -%>
includepkgs=<%= @include.to_a.join(' ') %>
<% end -%>
