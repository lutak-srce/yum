[argo-<%= @type %>]
name=ARGO Product Repository
baseurl=http://rpm-repo.argo.grnet.gr/ARGO/<%= @type %>/centos6/
gpgcheck=0
enabled=1
priority=<%= @priority %>
<% unless @exclude.empty? -%>
exclude=<%= @exclude.to_a.join(' ') %>
<% end -%>
<% unless @include.empty? -%>
includepkgs=<%= @include.to_a.join(' ') %>
<% end -%>
