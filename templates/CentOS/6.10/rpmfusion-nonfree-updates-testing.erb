[rpmfusion-nonfree-updates-testing]
name=RPM Fusion for EL 6 - Nonfree - Test Updates
#baseurl=http://download1.rpmfusion.org/nonfree/el/updates/testing/6/$basearch/
mirrorlist=http://mirrors.rpmfusion.org/mirrorlist?repo=nonfree-el-updates-testing-6&arch=$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-el-6
priority=<%= @priority %>
<% if @exclude.to_s != '' -%>
exclude=<%= @exclude.to_a.join(' ') %>
<% end -%>
<% if @include.to_s != '' -%>
includepkgs=<%= @include.to_a.join(' ') %>
<% end -%>

[rpmfusion-nonfree-updates-testing-debuginfo]
name=RPM Fusion for EL 6 - Nonfree - Test Updates Debug
#baseurl=http://download1.rpmfusion.org/nonfree/el/updates/testing/6/$basearch/debug/
mirrorlist=http://mirrors.rpmfusion.org/mirrorlist?repo=nonfree-el-updates-testing-debug-6&arch=$basearch
<% if @debuginfo -%>
enabled=1
<% else -%>
enabled=0
<% end -%>
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-el-6

[rpmfusion-nonfree-updates-testing-source]
name=RPM Fusion for EL 6 - Nonfree - Test Updates Source
#baseurl=http://download1.rpmfusion.org/nonfree/el/updates/testing/6/SRPMS/
mirrorlist=http://mirrors.rpmfusion.org/mirrorlist?repo=nonfree-el-updates-testing-source-6&arch=$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-el-6

