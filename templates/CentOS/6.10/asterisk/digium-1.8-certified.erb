[digium-1.8-certified]
name=CentOS-$releasever - Digium 1.8-certified
baseurl=http://packages.digium.com/centos/$releasever/digium-1.8-certified/$basearch/
enabled=1
gpgcheck=0
#gpgkey=http://packages.digium.com/RPM-GPG-KEY-Digium
priority=<%= @priority %>
<% if @exclude.to_s != '' -%>
exclude=<%= @exclude.to_a.join(' ') %>
<% end -%>
<% if @include.to_s != '' -%>
includepkgs=<%= @include.to_a.join(' ') %>
<% end -%>
