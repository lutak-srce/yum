[UMD-4-updates]
name=UMD 4 updates (CentOS 7)
baseurl=http://repository.egi.eu/sw/production/umd/4/centos7/$basearch/updates
protect=1
enabled=1
# To use priorities you must have yum-priorities installed
gpgcheck=1
gpgkey=http://repository.egi.eu/sw/production/umd/UMD-RPM-PGP-KEY
priority=<%= @priority %>
<% if @exclude.to_s != '' -%>
exclude=<%= @exclude.to_a.join(' ') %>
<% end -%>
<% if @include.to_s != '' -%>
includepkgs=<%= @include.to_a.join(' ') %>
<% end -%>
