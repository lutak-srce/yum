[zfs-testing]
name=ZFS on Linux for EL7 - Testing
baseurl=http://archive.zfsonlinux.org/epel-testing/7/$basearch/
enabled=1
metadata_expire=7d
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-zfsonlinux

[zfs-testing-source]
name=ZFS on Linux for EL7 - Testing Source
baseurl=http://archive.zfsonlinux.org/epel-testing/7/SRPMS/
<% if @source -%>
enabled=1
<% else -%>
enabled=0
<% end -%>
gpgcheck=1
