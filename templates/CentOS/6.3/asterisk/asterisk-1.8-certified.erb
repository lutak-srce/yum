[asterisk-1.8-certified]
name=CentOS-$releasever - Asterisk 1.8-certified
baseurl=http://packages.asterisk.org/centos/$releasever/asterisk-1.8-certified/$basearch/
enabled=1
gpgcheck=0
#gpgkey=http://packages.asterisk.org/RPM-GPG-KEY-Digium
priority=<%= @priority %>
<% unless @exclude.empty? -%>
exclude=<%= @exclude.to_a.join(' ') %>
<% end -%>
<% unless @include.empty? -%>
includepkgs=<%= @include.to_a.join(' ') %>
<% end -%>
