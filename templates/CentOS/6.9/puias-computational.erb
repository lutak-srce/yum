[puias-computational]
name=PUIAS computational Base $releasever - $basearch
mirrorlist=http://www.puias.princeton.edu/data/puias/computational/$releasever/$basearch/mirrorlist
#baseurl=http://www.puias.princeton.edu/data/puias/computational/$releasever/$basearch
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-puias
priority=<%= @priority %>
<% unless @exclude.empty? -%>
exclude=<%= @exclude.to_a.join(' ') %>
<% end -%>
<% unless @include.empty? -%>
includepkgs=<%= @include.to_a.join(' ') %>
<% end -%>
