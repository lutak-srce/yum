[ius-dev]
name=IUS Community Packages for Enterprise Linux 6 - $basearch - Dev
#baseurl=https://dl.iuscommunity.org/pub/ius/development/Redhat/6/$basearch
mirrorlist=https://mirrors.iuscommunity.org/mirrorlist?repo=ius-el6-dev&arch=$basearch&protocol=http
failovermethod=priority
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/IUS-COMMUNITY-GPG-KEY
priority=<%= @priority %>
<% unless @exclude.empty? -%>
exclude=<%= @exclude.join(' ') %>
<% end -%>
<% unless @include.empty? -%>
includepkgs=<%= @include.join(' ') %>
<% end -%>

[ius-dev-debuginfo]
name=IUS Community Packages for Enterprise Linux 6 - $basearch - Dev Debug Info
#baseurl=https://dl.iuscommunity.org/pub/ius/development/Redhat/6/$basearch/debuginfo
mirrorlist=https://mirrors.iuscommunity.org/mirrorlist?repo=ius-el6-dev-debuginfo&arch=$basearch&protocol=http
failovermethod=priority
<% if @debuginfo -%>
enabled=1
<% else -%>
enabled=0
<% end -%>
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/IUS-COMMUNITY-GPG-KEY
priority=<%= @priority %>
<% unless @exclude.empty? -%>
exclude=<%= @exclude.join(' ') %>
<% end -%>
<% unless @include.empty? -%>
includepkgs=<%= @include.join(' ') %>
<% end -%>

[ius-dev-source]
name=IUS Community Packages for Enterprise Linux 6 - $basearch - Dev Source
#baseurl=https://dl.iuscommunity.org/pub/ius/development/Redhat/6/SRPMS
mirrorlist=https://mirrors.iuscommunity.org/mirrorlist?repo=ius-el6-dev-source&arch=source&protocol=http
failovermethod=priority
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/IUS-COMMUNITY-GPG-KEY
priority=<%= @priority %>
<% unless @exclude.empty? -%>
exclude=<%= @exclude.join(' ') %>
<% end -%>
<% unless @include.empty? -%>
includepkgs=<%= @include.join(' ') %>
<% end -%>
